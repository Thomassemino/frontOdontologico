{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "https://consultorio-odontologico-backend-production-b1c8.up.railway.app/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }
